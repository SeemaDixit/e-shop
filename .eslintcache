RT_HIDDEN:\n            return {\n                ...state,\n                hidden: !state.hidden\n\n            }\n        case CartActionTypes.ADD_ITEM:\n            return {\n                ...state,\n                cartItems: addItemToCart(state.cartItems, action.payload)\n            }\n        default:\n            return state;\n    }\n}\n\nexport default cartReducer;","/Users/seemadixit/projects/node-apps/online-store/src/redux/cart/cart.utils.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/cart-item/cart-item.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/redux/cart/cart.selectors.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/redux/user/user.selector.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/pages/checkout/checkout.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/checkout-item/checkout-item.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":19,"column":7,"nodeType":"151","messageId":"152","endLine":19,"endColumn":15},{"ruleId":"149","severity":1,"message":"153","line":11,"column":28,"nodeType":"151","messageId":"152","endLine":11,"endColumn":48},{"ruleId":"149","severity":1,"message":"153","line":8,"column":27,"nodeType":"151","messageId":"152","endLine":8,"endColumn":47},{"ruleId":"149","severity":1,"message":"154","line":1,"column":10,"nodeType":"151","messageId":"152","endLine":1,"endColumn":17},"no-native-reassign",["155"],"no-negated-in-lhs",["156"],"no-unused-vars","'HatsPage' is assigned a value but never used.","Identifier","unusedVar","'selectCartItemsCount' is defined but never used.","'addItem' is defined but never used.","no-global-assign","no-unsafe-negation"]