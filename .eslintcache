h":"140","messages":"141","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"81"},{"filePath":"142","messages":"143","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"144","messages":"145","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"146","messages":"147","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"148","messages":"149","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"150","messages":"151","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"152","messages":"153","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/seemadixit/projects/node-apps/online-store/src/index.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/App.js",["154"],"import React from 'react';\nimport { Switch, Route, Redirect } from 'react-router-dom';\nimport { connect } from 'react-redux';\nimport { createStructuredSelector} from 'reselect';\nimport { auth, createUserProfileDocument } from './firebase/firebase.util';\n\nimport Homepage from './pages/homepage/homepage';\nimport ShopPage from './pages/shop/shop';\nimport SigninAndSignupPage from './pages/signin-signup/signin-signup';\nimport CheckoutPage from './pages/checkout/checkout';\n\nimport Header from './components/header/header';\nimport { setCurrentUser } from './redux/user/user.actions';\nimport  { selectCurrentUser} from './redux/user/user.selector'\n\nimport './App.css';\n\n\nconst HatsPage = () => (\n  <div>\n    <h1>HATS PAGE</h1>\n  </div>\n)\n\nclass App extends React.Component {\n  unsubscribeFromAuth = null;\n\n  componentWillUnmount() {\n    this.unsubscribeFromAuth();\n  }\n\n  componentDidMount() {\n\n    const { setCurrentUser } = this.props;\n    this.unsubscribeFromAuth = auth.onAuthStateChanged(async userAuth => {\n      //this.setState({ currentUser: user });\n      if(userAuth) {\n        const userRef = await createUserProfileDocument(userAuth);\n\n        userRef.onSnapshot(snapShot => {\n          setCurrentUser ({\n              id: snapShot.id,\n              ...snapShot.data()\n          });\n        });\n      }\n      setCurrentUser(userAuth);\n    });\n  }\n\n\n  render() {\n    return (\n      <div>\n        <Header/>\n        <Switch>\n          <Route exact={true} path='/' component={Homepage} />\n          <Route path='/shop' component={ShopPage} />\n          <Route exact={true} path='/checkout' component={CheckoutPage} />\n          <Route exact path='/signin' render={()=> \n              this.props.currentUser ? (\n              <Redirect to='/' />\n              ) : (\n              <SigninAndSignupPage />\n              )\n            } \n          />\n        </Switch>\n      </div>\n    );\n  }\n}\n\n const mapStateToProps = createStructuredSelector({\n   currentUser: selectCurrentUser\n });\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n})\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n",["155","156"],"/Users/seemadixit/projects/node-apps/online-store/src/reportWebVitals.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/pages/homepage/homepage.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/directory/directory.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/menu-item/menu-item.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/pages/shop/shop.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/pages/shop/shopData.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/collection-preview/collection-preview.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/collection-item/collection-item.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/header/header.js",["157"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { connect } from 'react-redux'; \nimport {createStructuredSelector } from 'reselect';\n\nimport { auth } from '../../firebase/firebase.util';\nimport { ReactComponent as Logo } from '../../assets/crown.svg';\nimport CartIcon from '../cart-icon/cart-icon';\nimport CartDropdown from '../cart-dropdown/cart-dropdown';\n\nimport { selectCartHidden, selectCartItemsCount} from '../../redux/cart/cart.selectors';\nimport { selectCurrentUser } from '../../redux/user/user.selector';\n\n\nimport './header.scss';\n\nconst Header = ({ currentUser, hidden }) => {\n    return (\n        <div className='header'>\n            <Link to='/' className='logo-container'>\n                <Logo className='logo' />\n            </Link>\n            <div className='options'>\n                <Link to='shop' className='option'>\n                    SHOP\n                </Link>\n                <Link to='shop' className='option'>\n                    CONTACT\n                </Link>\n                {\n                    currentUser ?\n                    <div className='option' onClick={() => auth.signOut()}>SIGN OUT</div>\n                    :\n                    <Link className='option' to='/signin'>SIGN IN</Link>\n                }\n                <CartIcon />\n            </div>\n            {\n                hidden ? null : <CartDropdown />\n            }\n        </div>\n    )\n}\n\nconst mapStateToProps = createStructuredSelector({\n     currentUser: selectCurrentUser,\n     hidden: selectCartHidden\n})\n\nexport default connect(mapStateToProps)(Header);","/Users/seemadixit/projects/node-apps/online-store/src/pages/signin-signup/signin-signup.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/signin/signin.js",[],"/Users/seemadixit/projects/node-apps/online-store/src/components/form-input/form-input.js",[],"/Users/seemadixit/projects/node-apps/online-s